<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="SeparatorBrush" Color="#C6BDBF"/>
    <LinearGradientBrush x:Key="InfoBar.Background">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#7A1F2D" Offset="0.4"/>
            <GradientStop Color="#A52A2A" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Highlight" Color="Red"/>
    <SolidColorBrush x:Key="Text.Foreground" Color="#535353"/>
    <SolidColorBrush x:Key="Text.Background" Color="#D9D9D9"/>
    <SolidColorBrush x:Key="MainText.Foreground" Color="#BD1414"/>
    <SolidColorBrush x:Key="SecondText.Foreground" Color="#930C0C"/>
</ResourceDictionary>