<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="WindowBackgroundColor">#202020</Color>
    <Color x:Key="TitleBarBackgroundColor">#181818</Color>
    <Color x:Key="TitleBarButtonColor">#948F90</Color>
    <Color x:Key="WorkAreaBackgroundColor">#2E2E2E</Color>
    <Color x:Key="TextBoxBackgroundColor">#383838</Color>
    <Color x:Key="HeaderTextBackgroundColor">#ABABAB</Color>
    <Color x:Key="SeparatorColor">#424242</Color>
    <Color x:Key="TextColor">#D6D6D6</Color>
    <Color x:Key="SecondaryTextColor">#A52A2A</Color>
    <Color x:Key="AccentColor">#A52A2A</Color>
    <Color x:Key="ButtonTextColor">#D6D6D6</Color>
    
    <Color x:Key="SearchNavigationButtonTextColor">#D6D6D6</Color>
    <Color x:Key="SearchNavigationButtonHoverColor">#3A3A3A</Color>
    <Color x:Key="SearchNavigationButtonPressedColor">#505050</Color>
    
    <Color x:Key="ProfileNavigationButtonTextColor">#D6D6D6</Color>
    <Color x:Key="ProfileNavigationButtonHoverColor">#3A3A3A</Color>
    <Color x:Key="ProfileNavigationButtonPressedColor">#505050</Color>
    
    <Color x:Key="GridLineChartColor">#000</Color>

    <Color x:Key="MainComboBoxTextColor">#D6D6D6</Color>
    <Color x:Key="MainComboBoxBackgroundColor">#3A3A3A</Color>
    <Color x:Key="MainComboBoxToggleButtonColor">#948F90</Color>
    <Color x:Key="MainComboBoxBackgroundHoverButtonColor">#505050</Color>
    <Color x:Key="MainComboBoxBorderColor">#424242</Color>

    <Color x:Key="ComponentCalculationHeaderComboBoxTextColor">#D6D6D6</Color>
    <Color x:Key="ComponentCalculationHeaderComboBoxBackgroundColor">#181818</Color>
    <Color x:Key="ComponentCalculationHeaderComboBoxToggleButtonColor">#948F90</Color>
    <Color x:Key="ComponentCalculationHeaderComboBoxBackgroundHoverButtonColor">#505050</Color>
    <Color x:Key="ComponentCalculationHeaderComboBoxBorderColor">#424242</Color>

    <Color x:Key="ProfileComboBoxTextColor">#D6D6D6</Color>
    <Color x:Key="ProfileComboBoxBackgroundColor">#3A3A3A</Color>
    <Color x:Key="ProfileComboBoxToggleButtonColor">#948F90</Color>
    <Color x:Key="ProfileComboBoxBackgroundHoverButtonColor">#505050</Color>
    <Color x:Key="ProfileComboBoxBorderColor">#424242</Color>

    <Color x:Key="SupplyMonitoringHeaderBackgroundColor">#D6D6D6</Color>
    <Color x:Key="SupplyMonitoringHeaderTextColor">#202020</Color>
    
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="TitleBarBackgroundBrush" Color="{StaticResource TitleBarBackgroundColor}"/>
    <SolidColorBrush x:Key="TitleBarButtonBrush" Color="{StaticResource TitleBarButtonColor}"/>
    <SolidColorBrush x:Key="WorkAreaBackgroundBrush" Color="{StaticResource WorkAreaBackgroundColor}"/>
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="{StaticResource TextBoxBackgroundColor}"/>
    <SolidColorBrush x:Key="SeparatorBrush" Color="{StaticResource SeparatorColor}"/>
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}"/>
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="{StaticResource SecondaryTextColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="ButtonTextBrush" Color="{StaticResource ButtonTextColor}"/>
    
    <SolidColorBrush x:Key="SearchNavigationButtonTextBrush" Color="{StaticResource SearchNavigationButtonTextColor}"/>
    <SolidColorBrush x:Key="SearchNavigationButtonHoverBrush" Color="{StaticResource SearchNavigationButtonHoverColor}"/>
    <SolidColorBrush x:Key="SearchNavigationButtonPressedBrush" Color="{StaticResource SearchNavigationButtonPressedColor}"/>
    
    <SolidColorBrush x:Key="ProfileNavigationButtonTextBrush" Color="{StaticResource ProfileNavigationButtonTextColor}"/>
    <SolidColorBrush x:Key="ProfileNavigationButtonHoverBrush" Color="{StaticResource ProfileNavigationButtonHoverColor}"/>
    <SolidColorBrush x:Key="ProfileNavigationButtonPressedBrush" Color="{StaticResource ProfileNavigationButtonPressedColor}"/>
    
    <SolidColorBrush x:Key="MainComboBoxTextBrush" Color="{StaticResource MainComboBoxTextColor}"/>
    <SolidColorBrush x:Key="MainComboBoxBackgroundBrush" Color="{StaticResource MainComboBoxBackgroundColor}"/>
    <SolidColorBrush x:Key="MainComboBoxToggleButtonBrush" Color="{StaticResource MainComboBoxToggleButtonColor}"/>
    <SolidColorBrush x:Key="MainComboBoxBackgroundHoverButtonBrush" Color="{StaticResource MainComboBoxBackgroundHoverButtonColor}"/>
    <SolidColorBrush x:Key="MainComboBoxBorderBrush" Color="{StaticResource MainComboBoxBorderColor}"/>

    <SolidColorBrush x:Key="ComponentCalculationHeaderComboBoxTextBrush" Color="{StaticResource ComponentCalculationHeaderComboBoxTextColor}"/>
    <SolidColorBrush x:Key="ComponentCalculationHeaderComboBoxBackgroundBrush" Color="{StaticResource ComponentCalculationHeaderComboBoxBackgroundColor}"/>
    <SolidColorBrush x:Key="ComponentCalculationHeaderComboBoxToggleButtonBrush" Color="{StaticResource ComponentCalculationHeaderComboBoxToggleButtonColor}"/>
    <SolidColorBrush x:Key="ComponentCalculationHeaderComboBoxBackgroundHoverButtonBrush" Color="{StaticResource ComponentCalculationHeaderComboBoxBackgroundHoverButtonColor}"/>
    <SolidColorBrush x:Key="ComponentCalculationHeaderComboBoxBorderBrush" Color="{StaticResource ComponentCalculationHeaderComboBoxBorderColor}"/>

    <SolidColorBrush x:Key="ProfileComboBoxTextBrush" Color="{StaticResource ProfileComboBoxTextColor}"/>
    <SolidColorBrush x:Key="ProfileComboBoxBackgroundBrush" Color="{StaticResource ProfileComboBoxBackgroundColor}"/>
    <SolidColorBrush x:Key="ProfileComboBoxToggleButtonBrush" Color="{StaticResource ProfileComboBoxToggleButtonColor}"/>
    <SolidColorBrush x:Key="ProfileComboBoxBackgroundHoverButtonBrush" Color="{StaticResource ProfileComboBoxBackgroundHoverButtonColor}"/>
    <SolidColorBrush x:Key="ProfileComboBoxBorderBrush" Color="{StaticResource ProfileComboBoxBorderColor}"/>

    <SolidColorBrush x:Key="GridLineChartBrush" Color="{StaticResource GridLineChartColor}"/>

    <SolidColorBrush x:Key="SupplyMonitoringBackgroundHeaderBrush" Color="{StaticResource SupplyMonitoringHeaderBackgroundColor}"/>
    <SolidColorBrush x:Key="SupplyMonitoringTextHeaderBrush" Color="{StaticResource SupplyMonitoringHeaderTextColor}"/>
    
    <LinearGradientBrush x:Key="MainGradientBackground">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#7A1F2D" Offset="0.4"/>
            <GradientStop Color="#A52A2A" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>